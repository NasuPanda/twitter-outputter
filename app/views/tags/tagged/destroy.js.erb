<% if @error_messages.blank? %>
  $("#tagged-tag-container").html("<%= j(render("pages/homes/tagged_tags")) %>");
  // コードからイベントを発火させる
  document.getElementById("editing-post-content").dispatchEvent(new Event("input"));
<% else %>
  alert("<%= join_words_with_newline(@error_messages) %>");
<% end %>